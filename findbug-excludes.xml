<?xml version="1.0" encoding="UTF-8"?>
<!--
	Error exclusion configuration.
	Some errors generated by findbug can contradict team code conventions or cannot be relevant for the project. This configuration
	allows to ignore such errors keeping findbug error report clean.
	Please do not misuse this configuration by ignoring errors that can be fixed
-->
<FindBugsFilter>
	<Match>
		<Bug category="I18N"/>
		<Bug pattern="DM_DEFAULT_ENCODING, DM_CONVERT_CASE"/>
	</Match>

	<Match>
		<Bug category="STYLE"/>
		<Bug pattern="BC_UNCONFIRMED_CAST_OF_RETURN_VALUE"/>
	</Match>
</FindBugsFilter>